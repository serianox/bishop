{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "array-type": false,
        "arrow-parens": false,
        "interface-name": false,
        "max-line-length": false,
        "max-classes-per-file": false,
        "member-ordering": [
            true,
            {
                "alphabetize": false
            }
        ],
        "no-console": false,
        "typedef": false
    },
    "rulesDirectory": []
}