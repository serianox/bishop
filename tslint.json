{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "array-type": false,
        "arrow-parens": false,
        "interface-name": false,
        "max-line-length": false,
        "max-classes-per-file": false,
        "member-ordering": [
            true,
            {
                "alphabetize": false
            }
        ],
        "no-console": false,
        "one-line": [
            true,
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "typedef": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}